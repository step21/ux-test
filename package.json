{
  "name": "factorial-webapp",
  "version": "1.0.0",
  "description": "A simple factorial calculator web app with tests.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start2": "node server2.js",
    "test": "wdio wdio.conf.js",
    "serve-and-test": "concurrently --kill-others \"npm start\" \"sleep 5 && npm run start2\"  \"sleep 5 && npm test\"",
    "wdio": "wdio run ./wdio.conf.js"
  },
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@testing-library/webdriverio": "^3.2.1",
    "@wdio/cli": "^8.32.1",
    "@wdio/dot-reporter": "^8.31.1",
    "@wdio/local-runner": "^8.32.1",
    "@wdio/mocha-framework": "^8.31.1",
    "@wdio/spec-reporter": "^8.31.1",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "concurrently": "^6.0.0",
    "mocha": "^8.3.2",
    "wdio-wait-for": "^3.0.11",
    "webdriverio": "^8.32.1"
  }
}
